            <Setter Property="Stroke" Value="Blue"/>
            <Setter Property="StrokeThickness" Value="3"/>
            <Setter Property="Fill" Value="MintCream"/>
        </Style>
        <Style x:Key="FireCell" TargetType="Rectangle">
            <Setter Property="Stroke" Value="DarkCyan"/>
            <Setter Property="StrokeThickness" Value="1"/>
            <Setter Property="Fill" Value="Yellow"/>
        </Style>
        <Style x:Key="DeadShipCell" TargetType="Rectangle">
            <Setter Property="Stroke" Value="Blue"/>
            <Setter Property="StrokeThickness" Value="3"/>
            <Setter Property="Fill" Value="Red"/>
        </Style>
        <Style x:Key="MissedCell" TargetType="Rectangle">
            <Setter Property="Stroke" Value="DarkCyan"/>
            <Setter Property="StrokeThickness" Value="1"/>
            <Setter Property="Fill" Value="DarkCyan"/>
        </Style>
