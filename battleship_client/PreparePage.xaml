            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="{Binding ElementName=LayoutRoot, Path=ActualHeight}"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
